page{
  display: flex;
  height: 100vh;
  flex-direction: column;
}
.map-box{
  flex: 1;
  width: 100%;
}
#myMap{
  width: 100%;
  height: 100%;
  position: relative;
}
cover-image,cover-view{
  position: absolute;

}
.map-icon{
  right: 20rpx;
  width: 76rpx;
  height: 76rpx;
}
.map-icon_msg{
  bottom: 242rpx;
}
.map-icon_user{
  bottom: 142rpx;
}
.map-icon_reset{
  bottom: 42rpx;
}
.detail{
  /* display: inline-block; */
  position: relative;
  font-size: 16px;
  padding:20px 0 0 40rpx;
}
.detail>.addr {
  font-size: 14px;
  color: #666;
  padding-top: 10rpx;
}
.detail .go {
  position: absolute;
  right: 45rpx;
  width: 85rpx;
  height: 90rpx;
  top: 60rpx;
}
